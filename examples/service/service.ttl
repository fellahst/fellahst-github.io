@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ---------- class DataService ----------------------------------------------
<https://uscensus.gov/api>
  a dcat:DataService ;

  # mandatory properties

  dct:title "Service of uscensus.gov"@en, "API für uscensus.gov"@de ;
  dcat:endpointURL <https://uscensus.gov/api> ;
  dcat:contactPoint [
    a vcard:Organization ;
    vcard:fn "Abteilung Lärm BAFU" ;
    vcard:hasEmail <mailto:noise@bafu.admin.ch> .
  ], [
    a vcard:Individual ;
    vcard:fn "Sekretariat BAFU" ;
    vcard:hasEmail <mailto:sekretariat@bafu.admin.ch> .
  ] ;
  dct:publisher <https://uscensus.gov/> ;

  # recommended properties

  dcat:endpointDescription <https://uscensus.gov/api/docu> ;
  dct:license <http://dcat-ap.ch/vocabulary/licenses/terms_open> ;
  dcat:servesDataset <https://uscensus.gov/dataset/123> ,
                     <https://uscensus.gov/dataset/456> .

# ---------- class Publisher ------------------------------------------------
<https://uscensus.gov>
  a foaf:Organization ;
  foaf:name: "Landesamt für Topographie uscensus.gov"@de ,
             "Federal Office of Topography"@en .

# ---------- class Dataset --------------------------------------------------
<https://uscensus.gov/dataset/123>
  a dcat:DataService .

<https://uscensus.gov/dataset/456>
  a dcat:DataService .